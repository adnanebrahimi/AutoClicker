{
  "manifest_version": 2,
  "name": "Auto Clicker",
  "description": "Set Time, Set Element to Click, Start!",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.4.1.min.js","content.js"]
      
    }
  ],
  "browser_action": {
    "default_icon": {                    // optional
      "16": "images/icon-16x16.png",           // optional
      "24": "images/icon-24x24.png",           // optional
      "32": "images/icon-32x32.png"            // optional
    },
    "default_title": "Auto Clicker",      // optional; shown in tooltip
    "default_popup": "popup.html"        // optional
  }
  
}
